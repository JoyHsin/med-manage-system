{
  "name": "med-manage-system-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "build:prod-analyze": "NODE_ENV=production ANALYZE=true npm run build",
